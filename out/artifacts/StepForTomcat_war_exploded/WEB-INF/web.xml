<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.login.servlet.LoginServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
<!-- 登录模块   -->

    <servlet>
        <servlet-name>CodeServlet</servlet-name>
        <servlet-class>com.login.servlet.CodeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CodeServlet</servlet-name>
        <url-pattern>/code</url-pattern>
    </servlet-mapping>
<!--    验证码生成模块-->

    <servlet>
        <servlet-name>CheckCodeServlet</servlet-name>
        <servlet-class>com.checkcode.CheckCodeServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>CheckCodeServlet</servlet-name>
        <url-pattern>/checkCode</url-pattern>
    </servlet-mapping>
<!--    验证码校验模块-->

    <servlet>
        <servlet-name>disposename</servlet-name>
        <servlet-class>com.disposename.DisposeNameServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>disposename</servlet-name>
        <url-pattern>/disposename</url-pattern>
    </servlet-mapping>
<!--通过id找用户名-->

    <servlet>
        <servlet-name>Checkname</servlet-name>
        <servlet-class>com.register.servlet.CheckNameServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Checkname</servlet-name>
        <url-pattern>/Checkname</url-pattern>
    </servlet-mapping>
<!--    获取名字检验是否存在数据库-->

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.register.servlet.RegisterServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
<!--    注册模块-->

    <servlet>
        <servlet-name>Myselfleft</servlet-name>
        <servlet-class>com.myself.left.servlet.MySelfLeftServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Myselfleft</servlet-name>
        <url-pattern>/myselfleft</url-pattern>
    </servlet-mapping>
<!--    个人主页左侧功能模块（TODO 可能撤去）-->

    <servlet>
        <servlet-name>exit</servlet-name>
        <servlet-class>com.exit.servlet.ExitServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>exit</servlet-name>
        <url-pattern>/exit</url-pattern>
    </servlet-mapping>
<!--    退出模块-->

    <servlet>
        <servlet-name>uploadimg</servlet-name>
        <servlet-class>com.myself.left.servlet.UpLoadingImgServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>uploadimg</servlet-name>
        <url-pattern>/uploadimg</url-pattern>
    </servlet-mapping>
<!--    上传图片模块-->

    <servlet>
        <servlet-name>photo</servlet-name>
        <servlet-class>com.myself.left.servlet.DisposeIdPhotoServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>photo</servlet-name>
        <url-pattern>/photo</url-pattern>
    </servlet-mapping>
<!--    请求图片模块-->

    <servlet>
        <servlet-name>postinvitation</servlet-name>
        <servlet-class>com.postapost.servlet.PostInvitationServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>postinvitation</servlet-name>
        <url-pattern>/postinvitation</url-pattern>
    </servlet-mapping>
<!--    发帖模块-->

    <servlet>
        <servlet-name>eachpostservlet</servlet-name>
        <servlet-class>com.postapost.servlet.EachPostServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>eachpostservlet</servlet-name>
        <url-pattern>/html</url-pattern>
        <url-pattern>/css</url-pattern>
        <url-pattern>/js</url-pattern>
        <url-pattern>/servlet</url-pattern>
        <url-pattern>/cookies</url-pattern>
        <url-pattern>/java</url-pattern>
        <url-pattern>/jdbc</url-pattern>
        <url-pattern>/jstl</url-pattern>
        <url-pattern>/vue</url-pattern>
        <url-pattern>/react</url-pattern>
        <url-pattern>/ajax</url-pattern>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>
<!--    查看所有帖子-->

    <servlet>
        <servlet-name>excuteurl</servlet-name>
        <servlet-class>com.showpost.servlet.ExcuteURL</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>excuteurl</servlet-name>
        <url-pattern>/html/content/*</url-pattern>
        <url-pattern>/css/content/*</url-pattern>
        <url-pattern>/js/content/*</url-pattern>
        <url-pattern>/servlet/content/*</url-pattern>
        <url-pattern>/cookies/content/*</url-pattern>
        <url-pattern>/java/content/*</url-pattern>
        <url-pattern>/jdbc/content/*</url-pattern>
        <url-pattern>/jstl/content/*</url-pattern>
        <url-pattern>/vue/content/*</url-pattern>
        <url-pattern>/react/content/*</url-pattern>
        <url-pattern>/ajax/content/*</url-pattern>
        <url-pattern>/about/content/*</url-pattern>
    </servlet-mapping>
<!--    查看帖子内容-->

    <servlet>
        <servlet-name>contentservlet</servlet-name>
        <servlet-class>com.showpost.servlet.ContentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>contentservlet</servlet-name>
        <url-pattern>/contentservlet</url-pattern>
    </servlet-mapping>
<!--    请求帖子正文内容-->

    <servlet>
        <servlet-name>postcommentservlet</servlet-name>
        <servlet-class>com.postcoment.servlet.PostCommentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>postcommentservlet</servlet-name>
        <url-pattern>/postcommentservlet</url-pattern>
    </servlet-mapping>
<!--    发表评论-->

    <servlet>
        <servlet-name>eachperson</servlet-name>
        <servlet-class>com.myself.left.servlet.Eeachperson</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>eachperson</servlet-name>
        <url-pattern>/space/*</url-pattern>
    </servlet-mapping>
<!--    个人空间-->

    <servlet>
        <servlet-name>SharesServlet</servlet-name>
        <servlet-class>com.shares.servlet.SharesServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SharesServlet</servlet-name>
        <url-pattern>/shareservlet</url-pattern>
    </servlet-mapping>
<!--    发表分享模块-->

    <servlet>
        <servlet-name>Modifypost</servlet-name>
        <servlet-class>com.postapost.servlet.Modifypost</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Modifypost</servlet-name>
        <url-pattern>/modifypost</url-pattern>
    </servlet-mapping>
<!--    修改帖子模块-->

    <servlet>
        <servlet-name>Delpost</servlet-name>
        <servlet-class>com.postapost.servlet.Delpost</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Delpost</servlet-name>
        <url-pattern>/delpost</url-pattern>
    </servlet-mapping>
<!--    删除帖子模块-->
    
    <servlet>
        <servlet-name>DelCommentServlet</servlet-name>
        <servlet-class>com.postcoment.servlet.DelCommentServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>DelCommentServlet</servlet-name>
        <url-pattern>/delcomment</url-pattern>
    </servlet-mapping>
<!--    删除评论模块-->

    <servlet>
        <servlet-name>ModifyProfile</servlet-name>
        <servlet-class>com.modifyprofile.servlet.ModifyProfile</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ModifyProfile</servlet-name>
        <url-pattern>/modifyprofile</url-pattern>
    </servlet-mapping>
<!--    修改个人信息模块-->
    
    <servlet>
        <servlet-name>ModifyPassword</servlet-name>
        <servlet-class>com.modifyprofile.servlet.ModifyPassword</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ModifyPassword</servlet-name>
        <url-pattern>/modifypassword</url-pattern>
    </servlet-mapping>
<!--    修改密码模块-->

    <servlet>
        <servlet-name>Isme</servlet-name>
        <servlet-class>com.myself.left.servlet.Isme</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Isme</servlet-name>
        <url-pattern>/isme</url-pattern>
    </servlet-mapping>
<!--    判断是否为自己-->
    
    <servlet>
        <servlet-name>SearchUsername</servlet-name>
        <servlet-class>com.myself.left.servlet.SearchUsername</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>SearchUsername</servlet-name>
        <url-pattern>/searchusername</url-pattern>
    </servlet-mapping>
<!--    搜索用户模块-->
    
    <servlet>
        <servlet-name>DisposeCommentServlet</servlet-name>
        <servlet-class>com.admin.servlet.AdminDisposeCommentServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>DisposeCommentServlet</servlet-name>
        <url-pattern>/admincomment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminDelcomment</servlet-name>
        <servlet-class>com.admin.servlet.AdminDelcomment</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminDelcomment</servlet-name>
        <url-pattern>/admindelcomment</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminDisposeUserServlet</servlet-name>
        <servlet-class>com.admin.servlet.AdminDisposeUserServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminDisposeUserServlet</servlet-name>
        <url-pattern>/adminuser</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminDelUser</servlet-name>
        <servlet-class>com.admin.servlet.AdminDelUser</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminDelUser</servlet-name>
        <url-pattern>/admindeluser</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AdminDisposeInvitationServlet</servlet-name>
        <servlet-class>com.admin.servlet.AdminDisposeInvitationServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>AdminDisposeInvitationServlet</servlet-name>
        <url-pattern>/admininvitation</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminDelInvitation</servlet-name>
        <servlet-class>com.admin.servlet.AdminDelInvitation</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminDelInvitation</servlet-name>
        <url-pattern>/admindelinvitation</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AdminDisposeAdminServlet</servlet-name>
        <servlet-class>com.admin.servlet.AdminDisposeAdminServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>AdminDisposeAdminServlet</servlet-name>
        <url-pattern>/adminserver</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminAddServlet</servlet-name>
        <servlet-class>com.admin.servlet.AdminAddServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminAddServlet</servlet-name>
        <url-pattern>/adminadd</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminDelServlet</servlet-name>
        <servlet-class>com.admin.servlet.AdminDelServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminDelServlet</servlet-name>
        <url-pattern>/admindel</url-pattern>
    </servlet-mapping>

</web-app>